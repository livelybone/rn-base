{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "svg": "node ./static/SvgDealProcess.js",
    "android": "npm run android:clean && react-native run-android && npm run svg",
    "ios": "npm run pod && react-native run-ios && npm run svg",
    "build:android": "cross-env NODE_ENV=production npm run svg && react-native bundle --platform android --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res --dev false",
    "build:ios": "cross-env NODE_ENV=production npm run svg && npm run pod && react-native bundle --platform ios --entry-file index.js --bundle-output ./ios/MyApp/main.jsbundle --assets-dest  ./bundles --dev false",
    "start": "react-native start",
    "test": "jest",
    "pod": "cd ios && pod install && cd ..",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "tsc": "yarn tsc",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@auraxy/redux-usage": "^1.0.7",
    "@livelybone/easy-request": "^1.8.0",
    "@livelybone/form": "^2.10.4",
    "@livelybone/react-form": "^2.0.0",
    "@livelybone/singleton": "^1.1.3",
    "@livelybone/storage": "^1.7.2",
    "@livelybone/use-async-data": "^1.1.2",
    "@livelybone/use-count-down": "^1.0.0",
    "@livelybone/use-force-update": "^1.0.1",
    "@livelybone/use-mounted": "^1.0.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.5.1",
    "@react-navigation/stack": "^5.5.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "date-fns": "^2.14.0",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-camera": "^3.30.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-safe-area-context": "^3.0.5",
    "react-native-screens": "^2.8.0",
    "react-native-svg": "^12.1.0",
    "react-native-webview": "8",
    "react-redux": "^7.2.0",
    "realm": "^6.0.2",
    "redux": "^4.0.5",
    "tinygradient": "^1.1.2",
    "union-tuple": "^1.1.2",
    "use-state-track-prop": "^1.3.0",
    "use-watch": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.37",
    "@types/react-native": "^0.62.13",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "@typescript-eslint/typescript-estree": "^3.3.0",
    "babel-jest": "^24.9.0",
    "chokidar": "^3.4.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "16.11.0",
    "typescript": "^3.9.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
